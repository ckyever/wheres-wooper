generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model session {
  id Int @id @default(autoincrement())
  target_ids String
  init_time DateTime @default(now())
  start_time DateTime?
}

model highscore {
  id Int @id @default(autoincrement())
  username String? @db.Char(20)
  time BigInt
  date DateTime @default(now())
}